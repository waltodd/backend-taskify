{
    "version": 2,
    "builds": [
      {
        "src": "index.js", // Replace with your main server file name
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js" // This should point to your main file
      }
    ],
    "env": {
      "MONGODB_URI": "@mongodb_uri", // Replace with your environment variable name
      "JWT_SECRET": "@jwt_secret" // Replace with your environment variable name
    }
  }
  